<!DOCTYPE html>
<html>
<head>
  <title>Guesser</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if flash.now[:errors] %>
  <ul class="alert alert-danger">
  <% flash.now[:errors].each do |error| %>
    <li><%= error %></li>
  <% end %>
  </ul>
<% end %>


<%= yield %>

</body>
</html>
